<h2>タグ作成</h2>
<div class="card p-3">
  <div>
    <%= form_with(model: @tag, url: admin_tags_path,local:true) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.submit '登録' %>
    <% end %>
  </div>
  <div class="card m-3">
    <h5>タグ一覧</h5>
    <% @tags.each do |tag| %>
      #<%= tag.name %>
    <% end %>
  </div>
</div>
